			<FillColor r="255" g="0" b="0" a="255"/>
			<StrokeColor r="255" g="0" b="0" a="255"/>
		</Ellipse> 
		<Sprite>
			<!-- <Image src="SoccerBall.png" x="0.0" y="0.0" height="5" width="5"> </Image>  -->
