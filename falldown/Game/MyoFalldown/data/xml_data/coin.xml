			density="0.12"
			friction="0.6"
			restitution="0.4"
			isSensor="true"
		>
			<Shape type="circle" x="0.0" y="0.0" radius="8.0"/>
		</Fixture>
	</RigidBody>
	
	<CoinEventHandler>
		<CoinCollected 
			scoreValue="150" 
			coinParameterName="coin" 
			scoreValueParameterName="scoreValue"
			coinCollectedSoundFile="sound_effects/Coin01.aif"
			rate="1.0"
			pan="0.0"
			amp="1.0"
			add="0.0"
		/>
		<DestroyCoin coinParameterName="coin"/>
		<LevelUp currentRiseSpeedParameterName="currentRiseSpeed"/>
	</CoinEventHandler>
</GameObject>