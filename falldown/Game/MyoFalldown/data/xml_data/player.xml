<GameObject>
	<Render>
		<Sprite>
			<Image src="images/SoccerBall.png" x="0.0" y="0.0" height="20.0" width="20.0"/>
		</Sprite>
	</Render>
	
	<RigidBody
		type="dynamic"
		linearDamping="0.0"
		angularDamping="0.01"
		gravityScale="1.3"
		allowSleep="false"
		awake="true"
		fixedRotation="true"
		bullet="false"
		active="true"
	>
		<Fixture
			density="0.12"
			friction="0.7"
			restitution="0.4"
			isSensor="false"
		>
			<Shape type="circle" x="0.0" y="0.0" radius="10.0"/>
		</Fixture>
		
		<OnCollideEvents>
			<Event collidedWith="coin" eventType="COIN_COLLECTED" coinParameterName="coin"/>
		</OnCollideEvents>
	</RigidBody>
	
	<PlayerController 
		acceleration="0.45" 
		maxSpeed="350.0" 
		jumpForce="0.24"
		jumpSoundFile="sound_effects/boing_spring.wav"
		rate="1.0"
		pan="0.0"
		amp="1.0"
		add="0.0"
		currentRiseSpeedParameterName="currentRiseSpeed"
	/>
</GameObject>