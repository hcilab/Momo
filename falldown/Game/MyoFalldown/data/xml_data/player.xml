<GameObject>
	<Render>
		<Sprite>
			<SpriteSheet src="images/stash.png" alphaImage=""
				x="0.0" y="0.0"  
				horzCount="16" vertCount="16" farmeFreq="1f"
				zOrder="0" height="128.0" scaleHeight="25"> 
			</SpriteSheet> 
		</Sprite> 
	</Render>
	
	<RigidBody
		type="dynamic"
		linearDamping="0.0"
		angularDamping="0.01"
		gravityScale="0.3"
		allowSleep="false"
		awake="true"
		fixedRotation="true"
		bullet="false"
		active="true"
	>
		<Fixture
			density="0.24"
			friction="0.925"
			restitution="0.1"
			isSensor="false"
		>
			<Shape type="circle" x="0.0" y="0.0" radius="10.0"/>
		</Fixture>
		
		<OnCollideEvents>
			<Event collidedWith="coin" eventType="COIN_COLLECTED" coinParameterName="coin"/>
		</OnCollideEvents>
	</RigidBody>
	
	<PlayerController
		acceleration="0.8"
		maxSpeed="350.0"
		minInputThreshold="0.3"
		leftSensitivity="1.5"
		rightSensitivity="1.5"
		jumpForce="3.52"
		jumpSoundFile="sound_effects/boing_spring.wav"
		rate="1.0"
		pan="0.0"
		amp="1.0"
		add="0.0"
		currentRiseSpeedParameterName="currentRiseSpeed"
	/>
</GameObject>