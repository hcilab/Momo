<GameObject>
	<Render>
		<Sprite>
			<SpriteSheet src="images/stash.png" alphaImage=""
				x="0.0" y="0.0"  
				horzCount="16" vertCount="16" farmeFreq="1f"
				zOrder="0" height="128.0" scaleHeight="30"> 
			</SpriteSheet> 
		</Sprite> 
	</Render>
	
	<RigidBody
		type="dynamic"
		linearDamping="0.0"
		angularDamping="0.01"
		gravityScale="4.0"
		allowSleep="false"
		awake="true"
		fixedRotation="true"
		bullet="false"
		active="true"
	>
		<Fixture
			density="0.12"
			friction="0.6"
			restitution="0.4"
			isSensor="false"
		>
			<Shape type="circle" x="0.0" y="0.0" radius="10.0"/>
		</Fixture>
		
		<OnCollideEvents>
			<Event collidedWith="coin" eventType="COIN_COLLECTED" coinParameterName="coin"/>
		</OnCollideEvents>
	</RigidBody>
	
	<PlayerController acceleration="10.0" maxSpeed="200.0" jumpForce="5.0" currentRiseSpeedParameterName="currentRiseSpeed"/>
</GameObject>